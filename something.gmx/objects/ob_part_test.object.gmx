            <string>//Where all the info was found / really good resource
//https://gamedevelopment.tutsplus.com/articles/mastering-the-gamemaker-studio-particle-system--cms-22782

                            //Particle System
//Create the particle system
test_particle_system = part_system_create()

//create particles
test_particle = part_type_create()

//Shape
part_type_shape(test_particle,pt_shape_square)

//Scale values for the particles
part_type_scale(test_particle,1,1)

//allowed sizes for the particles
part_type_size(test_particle,0.10,0.15,-0.001,0)

//The colours
part_type_colour1(test_particle,c_green)

//transparency
part_type_alpha2(test_particle,1,0.75)

//particle speeeeed
part_type_speed(test_particle,0.1,0.5,0,0)

//Direction the particles go
part_type_direction(test_particle,0,359,0,0)

//Gravity
part_type_gravity(test_particle,0.02,90)

//Orientation
part_type_orientation(test_particle,0,359,10,0,true)

//How long the particles live for
part_type_life(test_particle,100,150)

//Whether the particles will blend together
part_type_blend(test_particle,true)

                            //Particle Emitter
//Making the emitter
first_emitter = part_emitter_create(test_particle_system)

//Defining the emitter region
part_emitter_region(test_particle_system,first_emitter,x - 20,x + 20, y - 20, y + 20, ps_shape_ellipse,ps_distr_gaussian)

//Burst or stream
part_emitter_stream(test_particle_system,first_emitter,test_particle,1)



